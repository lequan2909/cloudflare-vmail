{
  "extends": ["astro/tsconfigs/strict", "../../tsconfig.json"],
  "compilerOptions": {
    "composite": true,
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"],
      "database": ["../../packages/database"]
    }
  },
  "references": [{ "path": "../../packages/database" }],
  "include": [".astro/types.d.ts", "**/*", "./worker-configuration.d.ts"],
  "exclude": ["dist", "node_modules", ".astro/cache"]
}
